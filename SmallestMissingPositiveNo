//Given an unsorted integer array nums, find the smallest missing positive integer.
#include<bits/stdc++.h>
using namespace std;
int max(int *a,int n){
    int m=a[0];
    for(int i=0;i<n;i++){
        if(a[i]>a[0])
        m=a[i];
    }
    return m;
}
int freq(int *a,int n){
    unordered_map<int,int>u;
    for(int i=0;i<n;i++)
    {
       u[a[i]]++;
    }
    for(auto i:u)
    {
        cout<<i.first<<" "<<i.second<<endl;
    }
}
int main(){
    int a[]={3,4,-1,1,1,3,2,41,2};
    cout<<freq(a,9);
}
